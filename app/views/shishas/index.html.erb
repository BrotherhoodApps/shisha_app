<h1>Listing shishas</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Description</th>
    <th>Contact</th>
    <th>Url</th>
    <th>Flavours</th>
    <th>Hours</th>
    <th>Facilities</th>
    <th>Image1</th>
    <th>Image2</th>
    <th>Image3</th>
    <th>Image4</th>
    <th>Isdetailed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shishas.each do |shisha| %>
  <tr>
    <td><%= shisha.name %></td>
    <td><%= shisha.address %></td>
    <td><%= shisha.latitude %></td>
    <td><%= shisha.longitude %></td>
    <td><%= shisha.description %></td>
    <td><%= shisha.contact %></td>
    <td><%= shisha.url %></td>
    <td><%= shisha.flavours %></td>
    <td><%= shisha.hours %></td>
    <td><%= shisha.facilities %></td>
    <td><%= shisha.image1 %></td>
    <td><%= shisha.image2 %></td>
    <td><%= shisha.image3 %></td>
    <td><%= shisha.image4 %></td>
    <td><%= shisha.isDetailed %></td>
    <td><%= link_to 'Show', shisha %></td>
    <td><%= link_to 'Edit', edit_shisha_path(shisha) %></td>
    <td><%= link_to 'Destroy', shisha, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

